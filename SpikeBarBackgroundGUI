import javax.swing.*;
import java.awt.*;

public class SpikeBarBackgroundGUI extends JPanel
{
    public void paintComponent(Graphics g)
    {
        //paints the spikes and rectangles
        Graphics2D g2 = (Graphics2D) g;
        SpikeBarBackground sBB = new SpikeBarBackground();
        sBB.draw(g2);
    }
}
