import javax.swing.;
import java.awt.*;
public class Spikes extends JPanel 
{
    private int []x={0, 20 ,40};
    private int []y= {0,25,0};
    public void paint(Graphics page)
    {
        for(int i=0;i<3000;i++)
        {
        x[0]=x[0]+40;
        x[1]=x[1]+40;
        x[2]=x[2]+40;
        
        page.setColor(Color.black);
        
        page.fillPolygon(x,y,x.length);
    }
}
}


